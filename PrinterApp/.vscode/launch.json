    "version": "0.2.0",
    "configurations": [
        {
            "type": "solidity",
            "request": "launch",
            "name": "Debug Solidity (via contract entry point)"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ContractListener Launch Program",
            "program": "${workspaceFolder}/ContractListener.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ServerLaunch Program",
            "program": "${workspaceFolder}/ServerApp.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
       
        {
            "type": "node",
            "request": "launch",
            "name": "BackendLaunch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/BackendApp.js"
        }
    ]
}